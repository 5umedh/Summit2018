<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="The template for a sample Tic-Tac-Toe application"
    jcr:primaryType="cq:Template"
    jcr:title="Simple Tic-Tac-Toe App"
    allowedPaths="[/content/spa-demo.*]">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="spa-demo/components/pages/tic-tac-toe-app">
        <mainContent jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <tic-tac-toe jcr:primaryType="nt:unstructured"
                sling:resourceType="spa-demo/components/content/tic-tac-toe"/>
        </mainContent>        
        <image jcr:primaryType="nt:unstructured">
            <file/>
        </image>        
    </jcr:content>
</jcr:root>
