<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/summit2018/settings/wcm/templates/content-page"
        jcr:primaryType="cq:PageContent"
        jcr:title="Events"
        sling:resourceType="summit2018/lab730/components/structure/page"
        pageTitle="Events"
        socialMedia="[facebook]">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <title
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Adobe Summit 2018"
                    sling:resourceType="summit2018/lab730/components/content/title"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/content/text"
                    text="Checkout the greatest events in Las Vegas"
                    textIsRich="true"/>
                <image
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/content/image"
                    fileReference="/content/dam/summit2018/summit-2015.jpg"/>
                <list
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/content/list"
                    childDepth="1"
                    linkItems="true"
                    listFrom="static"
                    pages="[/content/summit2018/en,/content/summit2018/fr]"
                    showDescription="false"
                    showModificationDate="false"
                    sortOrder="asc"
                    tagsMatch="any"/>
                <event
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/content/event"
                    eventId="1"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/form/container"
                    action="/content/usergenerated/summit2018/en/cq-gen1506071823762/"
                    actionType="foundation/components/form/actions/store">
                    <text
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Lorem"
                        sling:resourceType="summit2018/lab730/components/form/text"
                        name="lorem"
                        rows="2"
                        type="text"/>
                    <options
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Where does it come from?"
                        sling:resourceType="summit2018/lab730/components/form/options"
                        name="come-from"
                        source="local"
                        type="checkbox">
                        <items jcr:primaryType="nt:unstructured">
                            <item0
                                jcr:primaryType="nt:unstructured"
                                selected="false"
                                text="A"
                                value="a"/>
                            <item1
                                jcr:primaryType="nt:unstructured"
                                selected="false"
                                text="B"
                                value="b"/>
                        </items>
                    </options>
                    <button
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="summit2018/lab730/components/form/button"/>
                </container>
                <sharing
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="summit2018/lab730/components/content/sharing"/>
            </responsivegrid>
        </root>
    </jcr:content>
